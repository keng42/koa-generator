{
  "restartable": "rs",
  "ignore": [
    ".git",
    "node_modules/**/node_modules"
  ],
  "verbose": false,
  "execMap": {
    "js": "node --harmony"
  },
  "watch": [
    "app.js",
    "bin/",
    "config/*.js",
    "handles/*.js",
    "routes/*.js",
    "utils/*.js"
  ],
  "env": {
    "NODE_ENV": "development"
  },
  "ext": "js"
}
